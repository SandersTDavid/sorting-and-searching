 int[] data = new int[1];
 private void Bubblesort()
        {
            int counter, temp;
            bool swapped = true;
            while (swapped)

            {
                swapped = false;
                for (counter = 0; counter <= data.Length - 2; counter++)
                {

                    if (data[counter] > data[counter + 1])
                    {
                        temp = data[counter];
                        data[counter] = data[counter + 1];
                        data[counter + 1] = temp;
                        swapped = true;


                    }
                }
            }
        }
